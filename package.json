{
  "name": "theiceji-nova",
  "version": "2024.3.6",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jirayu-ninl/project-nova.git"
  },
  "author": "Jirayu Ninlapun",
  "bugs": {
    "url": "https://github.com/Jirayu-ninl/project-nova/issues"
  },
  "homepage": "https://server.TheIceJi.com",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"app/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "lint": "eslint \"{app,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:generate:acc": "prisma generate --accelerate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db": "yarn db:generate:acc && yarn db:push",
    "clean": "rimraf dist out"
  },
  "prisma": {
    "schema": "app/database/prisma/schema.prisma"
  },
  "main": "./dist/main.cjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    },
    "./generate": {
      "require": "./dist/generate.cjs",
      "types": "./dist/generate.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "generate": [
        "dist/generate.d.ts"
      ]
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/lib-storage": "^3.525.1",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "^5.10.2",
    "@prisma/extension-accelerate": "^1.0.0",
    "@prisma/generator-helper": "^5.10.2",
    "@sentry/node": "^7.105.0",
    "@sentry/profiling-node": "^7.105.0",
    "@sentry/tracing": "^7.105.0",
    "await-event-emitter": "^2.0.2",
    "bcryptjs": "^2.4.3",
    "filenamify": "^6.0.0",
    "flat": "^6.0.1",
    "get-relative-path": "^1.0.2",
    "graceful-fs": "^4.2.11",
    "ioredis": "^5.3.2",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "maath": "^0.10.7",
    "minio": "^7.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.11",
    "outmatch": "^0.7.0",
    "pluralize": "^8.0.0",
    "pupa": "^3.1.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "superjson": "^2.2.1",
    "ts-morph": "^21.0.1",
    "uuidv7": "^0.6.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@paljs/cli": "^6.0.7",
    "@paljs/plugins": "^6.0.7",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.5",
    "@swc/helpers": "^0.5.6",
    "@swc/register": "^0.1.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/flat": "^5.0.5",
    "@types/graceful-fs": "^4.1.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/pluralize": "^0.0.33",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "decimal.js": "^10.4.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "graphql-type-json": "^0.3.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "request": "^2.88.2",
    "rimraf": "^5.0.5",
    "simplytyped": "^3.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "temp-dir": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "yarn-upgrade-all": "^0.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "app",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
